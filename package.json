{
  "name": "twse-monitor",
  "displayName": "TWSE - Taiwan stock monitor",
  "description": "",
  "version": "0.0.1",
  "publisher": "pinshengjuan",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "台股",
    "Taiwan",
    "股票",
    "stock",
    "twse"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pinshengjuan/twse-monitor"
  },
  "activationEvents": [
    "onView:twse-monitor",
    "onCommand:twse-monitor.add",
    "onCommand:twse-monitor.item.remove"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "twse-monitor",
      "properties": {
        "twse-monitor": {
          "type": "object",
          "default": {},
          "description": "your watching list"
        }
      }
    },
    "views": {
      "explorer": [
        {
          "id": "twse-monitor",
          "name": "Where my money goes"
        }
      ]
    },
    "commands": [
      {
        "command": "twse-monitor.add",
        "title": "add",
        "icon": {
          "light": "media/light/add.svg",
          "dark": "media/dark/add.svg"
        }
      },
      {
        "command": "twse-monitor.item.remove",
        "title": "remove"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "twse-monitor.add",
          "when": "view == twse-monitor",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "twse-monitor.item.remove",
          "when": "view == twse-monitor"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "git-cz": "^4.7.6",
    "iconv-lite": "^0.6.3",
    "prettier": "^2.3.1"
  }
}
